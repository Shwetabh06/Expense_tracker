eCategories.map((iC) => iC.type).includes(formData.category)) {\n      setFormData({ ...formData, type: 'Income' });\n    } else if (expenseCategories.map((iC) => iC.type).includes(formData.category)) {\n      setFormData({ ...formData, type: 'Expense' });\n    }\n\n    setOpen(true);\n    addTransaction({ ...formData, amount: Number(formData.amount), id: uuidv4() });\n    setFormData(initialState);\n  };\n\n  const selectedCategories = formData.type === 'Income' ? incomeCategories : expenseCategories;\n\n  return (\n    <Grid container spacing={2}>\n      <Snackbar open={open} setOpen={setOpen} />\n      {/* <Grid item xs={12}>\n        <Typography align=\"center\" variant=\"subtitle2\" gutterBottom>\n          {segment ? (<> {segment.words.map((w) => w.value).join(\" \")} </>) : null } \n          {segment && segment.words.map((w) => w.value).join(' ')}\n          {segment ? <BigTranscript /> : 'Start adding transactions'} \n        </Typography>\n      </Grid> */}\n      <Grid item xs={12}>\n        <FormControl fullWidth>\n          <InputLabel>Type</InputLabel>\n          <Select value={formData.type} onChange={(e) => setFormData({ ...formData, type: e.target.value })}>\n            <MenuItem value=\"Income\">Income</MenuItem>\n            <MenuItem value=\"Expense\">Expense</MenuItem>\n          </Select>\n        </FormControl>\n      </Grid>\n      <Grid item xs={12}>\n        <FormControl fullWidth>\n          <InputLabel>Category</InputLabel>\n          <Select value={formData.category} onChange={(e) => setFormData({ ...formData, category: e.target.value })}>\n            {selectedCategories.map((c) => <MenuItem key={c.type} value={c.type}>{c.type}</MenuItem>)}\n          </Select>\n        </FormControl>\n      </Grid>\n\n      <Grid item xs={6}>\n        <TextField type=\"number\" label=\"Amount\" value={formData.amount} onChange={(e) => setFormData({ ...formData, amount: e.target.value })} fullWidth />\n      </Grid>\n      <Grid item xs={6}>\n        <TextField fullWidth label=\"Date\" type=\"date\" value={formData.date} onChange={(e) => setFormData({ ...formData, date: formatDate(e.target.value) })} />\n      </Grid>\n      <Button className={classes.button} variant=\"outlined\" color=\"primary\" fullWidth onClick={createTransaction}>Create</Button>\n    </Grid>\n  );\n};\n\nexport default NewTransactionForm;\n","/Users/shwetabhsmacbookpro/Downloads/Expenso-main/src/components/Main/List/List.jsx",[],"/Users/shwetabhsmacbookpro/Downloads/Expenso-main/src/constants/categories.js",[],"/Users/shwetabhsmacbookpro/Downloads/Expenso-main/src/components/Main/List/styles.js",[],"/Users/shwetabhsmacbookpro/Downloads/Expenso-main/src/components/Main/Form/styles.js",[],"/Users/shwetabhsmacbookpro/Downloads/Expenso-main/src/utils/formatDate.js",["111"],"export default (date) => {\n  const d = new Date(date);\n  let month = `${d.getMonth() + 1}`;\n  let day = `${d.getDate()}`;\n  const year = d.getFullYear();\n\n  if (month.length < 2) { month = `0${month}`; }\n  if (day.length < 2) { day = `0${day}`; }\n\n  return [year, month, day].join('-');\n};\n","/Users/shwetabhsmacbookpro/Downloads/Expenso-main/src/components/Snackbar/Snackbar.jsx",[],"/Users/shwetabhsmacbookpro/Downloads/Expenso-main/src/components/Snackbar/styles.js",[],{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","severity":1,"message":"117","line":21,"column":13,"nodeType":"118","endLine":21,"endColumn":24},{"ruleId":"116","severity":1,"message":"117","line":22,"column":13,"nodeType":"118","endLine":22,"endColumn":24},{"ruleId":"116","severity":1,"message":"117","line":23,"column":13,"nodeType":"118","endLine":23,"endColumn":24},{"ruleId":"119","severity":1,"message":"120","line":27,"column":11,"nodeType":"118","endLine":27,"endColumn":42},{"ruleId":"121","severity":1,"message":"122","line":3,"column":8,"nodeType":"123","messageId":"124","endLine":3,"endColumn":13},{"ruleId":"116","severity":1,"message":"117","line":16,"co